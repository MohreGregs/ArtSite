<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ArtSiteDashboard.Views.HomeView">
    <Grid>
        <StackPanel>
            <Button Click="Button_OnAddCharacter"><TextBlock Text="Add Character"/></Button>
            <Button Click="Button_OnAddArtist"><TextBlock Text="Add Artist"/></Button>
            <Button Click="Button_OnAddColor"><TextBlock Text="Add Color"/></Button>
            <Button Click="Button_OnAddArtwork"><TextBlock Text="Add Artwork"/></Button>
            <Button Click="Button_OnAddTag"><TextBlock Text="Add Tag"/></Button>
            <Button Click="Button_OnAddSpecies"><TextBlock Text="Add Species"/></Button>
            <Button Click="Button_OnAddHobby"><TextBlock Text="Add Hobby"/></Button>
            <Button Click="Button_OnAddSong"><TextBlock Text="Add Song"/></Button>
        </StackPanel>
    </Grid>
</UserControl>
